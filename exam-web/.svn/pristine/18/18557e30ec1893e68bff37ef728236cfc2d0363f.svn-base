* {
	margin: 0px;
	padding: 0px;
}

body {
	height: 100%;
}

.top-container {
	width: 100%;
	height:auto;
	border-bottom: 2px solid #0000FF;
	float: left;
}

.photo {
	margin: 10px;
	width: 90px;
	height:120px;
	border: 1px solid red;
	float: left;
}

.student_info {
	margin: 10px 0px 0px 50px;
	float: left;
	width: 50%;
	line-height: 28px;
	height: atuo;
}

.span_length {
	width: 25%;
	display: inline-block;
}

.left-container {
	width: 25%;
	height: auto;
	float: left;
}

.left-inner-container {
	width: 90%;
	margin: 0px;
	padding: 5%;
}

.indicate-banner {
	float: left;
	clear: both;
}
.indicate-banner span {
	float: left;
}
.indicate-grey {
	display: inline-block;
	float: left;
	width: 25px;
	height: 25px;
	background: #EAEAEA;
}

.indicate-green {
	display: inline-block;
	float: left;
	width: 25px;
	height: 25px;
	background: #33ff99;
}

.indicate-oranage {
	display: inline-block;
	float: left;
	width: 25px;
	height: 25px;
	background: #FF8C00;
}

.indicate-question {
	clear: both;
	height: auto;
	float: left;
	margin-top: 30px;
}

.question_module {
	clear: both;
	float: left;
	height: auto;
	margin-top: 10px;
	line-height: 28px;
	width: 100%;
}

.title_brief {
	clear: both;
	margin-top: 25px;
	margin-left: 5px;
	font-size: 20px;
	font-weight: bold;
	height: 30px;
	line-height: 30px;
}

.question-index-block {
	display: inline-block;
	width: 25px;
	height: 25px;
	text-align: center;
	line-height: 25px;
}

.question-index-block-grey-color {
	background: #EAEAEA;
}

.question-index-block-green-color {
	background: #33ff99;
}

.question-index-block-oranage-color {
	background: #FF8C00;
}

.right-container {
	width: 71%;
	padding: 1% 2%;
	height: auto;
	float: left;
	background: #EAEAEA;
}

.topic_div {
	height: 710px;
}

.question_show {
	display: block;
}

.question_hide {
	display: none
}

.option_block {
	clear: both;
	float: left;
	height: 30px;
	line-height: 30px;
	margin-left: 25px;
}

.option_dot {
	width: 20px;
	height: 20px;
	border-radius: 50%;
	-moz-border-radius: 50%;
	-webkit-border-radius: 50%;
	border: 1px solid red;
	float: left;
}

.option_selected {
	background: #33ff99;
}

.option_content {
	float: left;
	line-height: 25px;
	margin-left: 10px;
}

.control_button {
	clear: both;
	float: left;
	height: auto;
}

.submit_btn {
	float: left;
	width: 20%;
}

.form_style {
	widht: 100%;
}

.button_style {
	float: right;
	border: none;
	width: 105px;
	height: 35px;
	font-size: 20px;
	font-weight: bold;
	color: white;
	background-color: #FF8C00;
	-moz-border-radius: 10%;
	-webkit-border-radius: 10%;
	margin-top: 50px;
	cursor: pointer;
}

.question_block {
	height: auto;
}

.question_num {
	float: left;
	line-height: 40px;
	width: 35px;
}

.question_content {
	float: left;
	line-height: 40px;
}

.btn_style {
	float: left;
	border: none;
	width: 65px;
	height: 30px;
	font-size: 14px;
	background-color: white;
	-moz-border-radius: 5%;
	-webkit-border-radius: 5%;
	margin-left: 30px;
	cursor: pointer;
}